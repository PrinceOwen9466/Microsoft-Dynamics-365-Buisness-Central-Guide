<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="svg/ScalableGraphics.xaml"/>
    </ResourceDictionary.MergedDictionaries>
    
    <BitmapImage x:Key="MenuIcon" UriSource="Grid Launcher.png" />
    <BitmapImage x:Key="PushPinIcon" UriSource="Push Pin.png" />
    <BitmapImage x:Key="MinimizeIcon" UriSource="Minimize.png"/>
    <BitmapImage x:Key="CloseIcon" UriSource="Close.png"/>
    <BitmapImage x:Key="BusinessCentralLogo" UriSource="Business Central Logo.png"/>
    <BitmapImage x:Key="BusinessCentralLogoWhite" UriSource="Business Central Logo White.png"/>
    <BitmapImage x:Key="BusinessCentralLogoTeal" UriSource="Business Central Logo Teal.png"/>
    <BitmapImage x:Key="LeftArrow" UriSource="Left Arrow.png"/>
    <BitmapImage x:Key="RightArrow" UriSource="Right Arrow.png"/>
    

</ResourceDictionary>