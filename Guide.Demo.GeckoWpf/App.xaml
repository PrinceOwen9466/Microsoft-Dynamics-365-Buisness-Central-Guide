<prism:PrismApplication x:Class="Guide.Demo.GeckoWpf.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Guide.Demo.GeckoWpf"
             xmlns:controls="clr-namespace:Guide.Demo.GeckoWpf.Controls"
             xmlns:prism="http://prismlibrary.com/"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>

            <ControlTemplate x:Key="GeckoBrowserTemplate" TargetType="{x:Type controls:GeckoBrowser}">
                <Grid>
                    <WindowsFormsHost x:Name="PART_HOST"/>
                </Grid>
            </ControlTemplate>

        </ResourceDictionary>
    </Application.Resources>
</prism:PrismApplication>
